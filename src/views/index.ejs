<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
    <link rel='stylesheet' href='/stylesheets/index.css' />
    <script src="script/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="/script/index.js"></script>
    <script src="script/event.js"></script>
    <script src="script/tween.js"></script>

</head>

<body>
    <div class="search">
        <label>项目名称:</label>
        <input>
    </div>
    <div class="tabmain">
        <div id="outerWrap">
            <div id="sliderParent"></div>
            <ul class="tabGroup">
                <% body.forEach(item=>{ %>
                    <li class="tabOption">
                        <%=item.fileName %>
                    </li>
                <% }) %>
            </ul>
        </div>
        <div id="container">
            <div id="content">
                <% body.forEach(item=>{ %>
                    <div class="tabContent">
                        <% if(item.metadata){ %>
                            <div class="version">
                                <div class="name">文档版本信息</div>
                                <ul class="list">
                                    <% item.metadata.forEach(meta=>{ %>
                                        <li>
                                            <%= meta.name %>: <%= meta.value %>
                                        </li>
                                    <% }) %>
                                </ul>
                            </div>
                        <% } %>
                        <div class="resource">
                            <div class="name">模块：<%= item.name %></div>
                            <div class="detail">
                                <%- marked(item.description) %>
                            </div>
                        </div>
                        <% item.resourceGroups.forEach(resourceGroups=>{ %>
                            <% resourceGroups.resources.forEach(resources=>{ %>
                                <div class="resourcesName">资源:<%= resources.uriTemplate %></div>
                                <dl class="resourcesList">
                                    <% resources.actions.forEach(action=>{ %>
                                        <dt>
                                            <%= action.name %>
                                        </dt>
                                        <dd>
                                            <span>接口描述：</span>
                                            <%= action.description %>
                                        </dd>
                                        <dd>
                                            <span>请求方法：</span>
                                            <%= action.method %>
                                        </dd>
                                        <% action.examples.forEach(example=>{ %>
                                            <%= JSON.stringify(example.requests) %>
                                            <% example.responses.forEach(response=>{ %>
                                                <dd>
                                                    <span>返回结果： </span>
                                                    <pre><%= jsonParse(response.body) %></pre>
                                                </dd>
                                            <% }) %>
                                        <% }) %>
                                    <% }) %>
                                </dl>
                            <% }) %>
                        <% }) %>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
</body>

</html>

<script>
$.ajax({
    url:'/message',
    data:{
        name:'yang',age:25
    }
})
</script>